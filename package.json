{
  "name": "@jondotsoy/simplerpc",
  "description": "Make a RPC with typescript easily",
  "version": "0.1.2",
  "module": "index.ts",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./lib/esm/simplerpc.js",
      "types": "./lib/types/simplerpc.d.ts"
    },
    "./create-http-client": {
      "import": "./lib/esm/create-http-client.js",
      "types": "./lib/types/create-http-client.d.ts"
    },
    "./create-http-server": {
      "import": "./lib/esm/create-http-server.js",
      "types": "./lib/types/create-http-server.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@jondotsoy/flags": "^2.1.0",
    "@jondotsoy/utils-js": "^0.5.0",
    "artur": "^1.2.1",
    "yaml": "^2.5.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.3",
    "zod-to-ts": "^1.2.0"
  },
  "scripts": {
    "prepack": "make build",
    "fmt": "prettier -w ."
  }
}
